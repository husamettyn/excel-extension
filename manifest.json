{
  "manifest_version": 3,
  "name": "Excel Extension",
  "version": "1.0",
  "description": "A basic Chrome extension for interacting with Excel files.",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}